{
  "expo": {
    "name": "Gemini",
    "slug": "gemini",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#009999"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/push-icon.png",
      "color": "#009999",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "ios": {
      "supportsTablet": true
    },
    "description": "MBLGTACC's custom conference app. This allows for viewing the schedule, adding it to your personal schedule, viewing maps, submitting evaluations and more!"
  }
}
